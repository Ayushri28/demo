package project1;

import java.util.Scanner;

public class Marks {

	public static void main(String args[])
	{
		int n=0,sum=0;
		Scanner input= new Scanner(System.in);
		n= input.nextInt();
		int[] arr=new int[n];
		System.out.println("how many subjects");
		
		
		for(int i=1; i<=n; i++)
		{
			System.out.println("enter the marks");
			arr[i]=input.nextInt();
			sum+=arr[i];
		}
		
		System.out.println(sum);
	}
}
